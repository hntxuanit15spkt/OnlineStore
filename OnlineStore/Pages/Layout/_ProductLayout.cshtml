    <div class="product">
        <div class="container">
            <div class="col-md-3 product-price">
                <div class=" rsidebar span_1_of_left">
                    <div class="of-left">
                        <h4 class="cate">Danh mục sản phẩm </h4>
                    </div>
                    <ul class="menu">
                        <li class="item1">
                            <a href="#">Điện thoại </a>
                            <ul class="cute">
                                <li class="subitem1"><a href="#">Sony </a></li>
                                <li class="subitem2"><a href="#">HTC </a></li>
                                <li class="subitem3"><a href="#">Samsung</a></li>
                            </ul>
                        </li>
                        <li class="item2">
                            <a href="#">Máy tính bảng </a>
                            <ul class="cute">
                                <li class="subitem1"><a href="#">Apple</a></li>
                                <li class="subitem2"><a href="#">Asus </a></li>
                                <li class="subitem3"><a href="#">Samsung </a></li>
                            </ul>
                        </li>
                        <li class="item3">
